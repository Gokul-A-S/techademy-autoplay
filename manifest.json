{
    "manifest_version": 3,
    "name": "techademy-autoplay",
    "version": "1.0",
    "description": "Auto play videos on techademy",

    "content_scripts": [
        {
            "matches": ["*://ibm.techademy.com/*"],
            "js": ["main.js"]
        }
    ],
    
        "action": {
          "default_icon": {              
            "16": "images/icon.png",   
            "24": "images/icon.png",   
            "32": "images/icon.png"    
          },
          "default_title": "Open Controls",   
          "default_popup": "popup.html"  
        }
       

     


}